<Window x:Class="BilhusUI.AddCarToService"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BilhusUI"
        mc:Ignorable="d"
        Title="AddCarToService" Height="250" Width="400">
    <Grid>
        <ListBox x:Name="carList" Margin="10,10,10,67"/>
        <Button x:Name="addbtn" Content="Add" HorizontalAlignment="Left" Margin="11,172,0,0" VerticalAlignment="Top" Click="addbtn_Click"/>
        <Button x:Name="cancelbtn" Content="Cancel" HorizontalAlignment="Left" Margin="52,172,0,0" VerticalAlignment="Top" Click="cancelbtn_Click" Width="38"/>
    </Grid>
</Window>
