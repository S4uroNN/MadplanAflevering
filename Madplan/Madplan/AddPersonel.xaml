<Window x:Name="addPersonel" x:Class="BilhusUI.AddPersonel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BilhusUI"
        mc:Ignorable="d"
        Title="Add Personel" Height="286" Width="400">
    <Grid Margin="0,0,0,34">
        <TextBox x:Name ="txtName" HorizontalAlignment="Left" Margin="89,82,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name = "txtAge"  HorizontalAlignment="Left" Margin="89,117,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label Content="Name" Margin="31,77,316,88"/>
        <Label Content="Age" Margin="31,113,316,52"/>
        <Button x:Name="addbtn" Content="Add" HorizontalAlignment="Left" Margin="89,172,0,0" VerticalAlignment="Top" Width="46" Click="addbtn_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="155,172,0,0" VerticalAlignment="Top" Height="19" Width="54" Click="cancelBtn_Click"/>

    </Grid>
</Window>
